void main() {
 double result = calculateArea(12, 5);
 print ('The result is ' + result.toString());
}
//define function calculateArea
double calculateArea(double width, double height, [bool isTriangle]) {
 double area;
 if (isTriangle) {
 area = width * height / 2;
 }
 else {
 area = width * height;
 }
 return area; 
}
